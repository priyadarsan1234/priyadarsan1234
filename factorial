import java.util.Scanner;
class factorial
{
   public static void main(String args[])
   {
      int n;
      System.out.println("enter a number");
      Scanner sc=new Scanner(System.in);
      n=sc.nextInt();
      int i=1;
      int fact=1;
      while(i<=n)
      {
         fact=fact*i;
         i++; 
      }
      System.out.println("factorial no is"+fact);
   }
}
